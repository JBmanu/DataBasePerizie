        <Label x:Name="label1_Copy3" Content="Massimale:" HorizontalAlignment="Left" Margin="502,274,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txt_Massimale1Polizza" HorizontalAlignment="Left" Height="23" Margin="502,300,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.067,0.565"/>
