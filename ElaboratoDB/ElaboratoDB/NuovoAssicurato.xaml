        <Label x:Name="lbl_Assicurato_Copy5" Content="NUOVO ASSICURATO" Margin="331,12,385,440"/>
        <Label x:Name="lbl_NuovaPolizza" Content="Crea nuova polizza" HorizontalAlignment="Left" Margin="489,71,0,0" VerticalAlignment="Top" Width="133" FontSize="14" RenderTransformOrigin="0.303,-0.184"/>
        <Label x:Name="label1_Copy1" Content="Categoria:" HorizontalAlignment="Left" Margin="502,153,0,0" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="cmb_Categoria1Polizza" Margin="502,187,195,291"/>
